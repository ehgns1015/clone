{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:react/recommended", "prettier", "prettier/react"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "import"],
  "rules": {
    "react/prop-types": 0, // or "off"
    "no-unused-vars": "warn"
  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
